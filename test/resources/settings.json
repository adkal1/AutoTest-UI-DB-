                "safebrowsing.enabled": "true"
            },
            "startArguments": [
                
            ],
            "webDriverVersion": "102.0.5005.61"
        }
    },
    "elementCache": {
        "isEnabled": false
    },
    "isElementHighlightEnabled": true,
    "isRemote": false,
    "logger": {
        "language": "en",
        "logPageSource": true
    },
    "remoteConnectionUrl": "http://localhost:4444/wd/hub",
    "retry": {
        "number": 2,
        "pollingInterval": 300
    },
    "timeouts": {
        "timeoutCommand": 120,
        "timeoutCondition": 30,
        "timeoutImplicit": 0,
        "timeoutPageLoad": 15,
        "timeoutPollingInterval": 300,
        "timeoutScript": 10
    }
